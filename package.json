{
	"name": "obsidian-note-sharing",
	"version": "1.0.0",
	"description": "Obsidian plugin for encrypting and uploading notes to noteshare.space.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
