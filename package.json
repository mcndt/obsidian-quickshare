		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"builtin-modules": "^3.2.0",
		"c8": "^7.11.3",
		"esbuild": "0.13.12",
		"esbuild-svelte": "^0.7.1",
		"obsidian": "latest",
		"svelte": "^3.48.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "2.3.1",
		"typescript": "~4.5.0",
